{
  "name": "calc-rate",
  "version": "1.1.6",
  "description": "calculator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "zany",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/muguliebe/calc-stock"
  },
  "nodemonConfig": {
    "watch": [
      "dist/**/*",
      "test/norm/**/*"
    ]
  },
  "scripts": {
    "build": "tsc",
    "bw": "tsc --watch",
    "start": "node .",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit --maxWorker=1",
    "tw": "cross-env NODE_ENV=test jest --watchAll --runInBand --detectOpenHandles --forceExit --maxWorker=1",
    "prepublish": "tsc",
    "upgrade": "yarn upgrade-interactive --latest",
    "tt": "nodemon test/norm/tmp.test.js"
  },
  "dependencies": {
    "decimal.js": "^10.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "cross-env": "^7.0.3",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1"
  }
}
